<list>
  <com.axibase.collector.model.job.socrata.SocrataJobConfig>
    <name>socrata-consumerfinance</name>
    <cronExpression>0 0 8 * * ?</cronExpression>
    <enabled>true</enabled>
    <jobType>SOCRATA</jobType>
    <defaultTags/>
    <checkLastTime>false</checkLastTime>
    <commitInterval>10</commitInterval>
    <errorThreshold>0</errorThreshold>
    <configurations>
      <com.axibase.collector.model.job.socrata.SocrataConfiguration>
        <name>Consumer Complaints</name>
        <path>https://data.consumerfinance.gov/api/views/s6ew-h6mp</path>
        <ignoreDuplicates>true</ignoreDuplicates>
        <settings>
          <com.axibase.collector.model.job.socrata.SocrataConfigurationSetting>
            <entityField>$.meta.view.id</entityField>
            <timeEnabled>true</timeEnabled>
            <timeField>date_received</timeField>
            <timeFormat>yyyy-MM-dd&apos;T&apos;HH:mm:ss</timeFormat>
            <timeZone>UTC</timeZone>
            <seriesEnabled>true</seriesEnabled>
            <metricPrefix>CFPB.</metricPrefix>
            <includedFields>
              <string>*</string>
            </includedFields>
            <excludedFields>
              <string>company_public_response</string>
              <string>complaint_id</string>
              <string>date_sent_to_company</string>
              <string>tags</string>
              <string>zip_code</string>
              <string>complaint_what_happened</string>
            </excludedFields>
            <annotationFields/>
            <propertyEnabled>false</propertyEnabled>
            <propertyTypeField>$.meta.view.name</propertyTypeField>
            <propertyKeyFields/>
            <propertyValueFields/>
            <messageEnabled>false</messageEnabled>
            <messageTypeField>$.meta.view.name</messageTypeField>
            <messageSourceField>$.meta.view.id</messageSourceField>
            <messageTagFields>
              <string>*</string>
            </messageTagFields>
          </com.axibase.collector.model.job.socrata.SocrataConfigurationSetting>
        </settings>
        <customTags/>
        <addRowNumber>true</addRowNumber>
        <skipOldData>true</skipOldData>
        <checkEachRow>false</checkEachRow>
      </com.axibase.collector.model.job.socrata.SocrataConfiguration>
    </configurations>
  </com.axibase.collector.model.job.socrata.SocrataJobConfig>
</list>