<list>
  <com.axibase.collector.model.job.jmx.JmxJobConfig>
    <name>kafka-jmx</name>
    <cronExpression>R 0/2 * * * ?</cronExpression>
    <enabled>true</enabled>
    <jobType>JMX</jobType>
    <defaultTags/>
    <checkLastTime>false</checkLastTime>
    <commitInterval>10</commitInterval>
    <errorThreshold>0</errorThreshold>
    <configurations>
      <com.axibase.collector.model.job.jmx.JmxConfiguration>
        <name>kafka-series</name>
        <host>${ITEM?column(&apos;host&apos;)}</host>
        <port>${ITEM?column(&apos;port&apos;)}</port>
        <userName>${ITEM?column(&apos;user&apos;)}</userName>
        <password>MnfWy/fs9frIwJ5QvYTjQZfvBj1iGrPY/c4BdkOOVoQ=</password>
        <trackingMBeans>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.server:name=*,type=BrokerTopicMetrics</objectName>
            <attributes>OneMinuteRate,FiveMinuteRate,FifteenMinuteRate</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.controller:name=*,type=KafkaController</objectName>
            <attributes>Value</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.log:name=LogEndOffset,partition=*,topic!=_*,type=Log</objectName>
            <attributes>Value</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.cluster:name=UnderReplicated,partition=*,topic!=_*,type=Partition</objectName>
            <attributes>Value</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.server:name=BrokerState,type=KafkaServer</objectName>
            <attributes>Value</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.server:name=*,topic!=_*,type=BrokerTopicMetrics</objectName>
            <attributes>OneMinuteRate,FiveMinuteRate,FifteenMinuteRate</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
        </trackingMBeans>
        <entity>${ITEM?column(&apos;host&apos;)}</entity>
        <metricPrefix>jmx.${domain}.${type}.${name}.</metricPrefix>
        <propertyTypePrefix></propertyTypePrefix>
        <serviceName></serviceName>
        <excludedMetrics>
          <string>75thPercentile</string>
          <string>95thPercentile</string>
          <string>98thPercentile</string>
          <string>999thPercentile</string>
          <string>Count</string>
          <string>EventType</string>
          <string>LatencyUnit</string>
          <string>Max</string>
          <string>Min</string>
          <string>RateUnit</string>
          <string>StdDev</string>
        </excludedMetrics>
        <excludedProperties/>
        <predefinedTags/>
        <putType>SERIES</putType>
        <entitySet>
          <name>kafka-cluster-jmx</name>
          <type>TEXT</type>
          <discardDuplicates>false</discardDuplicates>
          <entities>
            <string>#host,port,user,password</string>
            <string>host1,port1,,</string>
            <string>host2,port2,,</string>
            <string>host3,port3,,</string>
          </entities>
          <separator>,</separator>
        </entitySet>
      </com.axibase.collector.model.job.jmx.JmxConfiguration>
      <com.axibase.collector.model.job.jmx.JmxConfiguration>
        <name>kafka-property</name>
        <host>${ITEM?column(&apos;host&apos;)}</host>
        <port>${ITEM?column(&apos;port&apos;)}</port>
        <userName>${ITEM?column(&apos;user&apos;)}</userName>
        <password>MnfWy/fs9frIwJ5QvYTjQZfvBj1iGrPY/c4BdkOOVoQ=</password>
        <trackingMBeans>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.server:name=*,type=KafkaServer</objectName>
            <attributes>*</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.server:type=app-info</objectName>
            <attributes>50thPercentile</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
          <com.axibase.collector.model.job.jmx.TrackingMBean>
            <objectName>kafka.controller:name=*,type=KafkaController</objectName>
            <attributes>50thPercentile</attributes>
            <propertyType></propertyType>
          </com.axibase.collector.model.job.jmx.TrackingMBean>
        </trackingMBeans>
        <entity>${ITEM?column(&apos;host&apos;)}</entity>
        <metricPrefix></metricPrefix>
        <propertyTypePrefix>jmx.kafka.</propertyTypePrefix>
        <serviceName></serviceName>
        <excludedMetrics>
          <string>75thPercentile</string>
          <string>95thPercentile</string>
          <string>98thPercentile</string>
          <string>999thPercentile</string>
          <string>EventType</string>
          <string>FifteenMinuteRate</string>
          <string>LatencyUnit</string>
          <string>RateUnit</string>
          <string>StdDev</string>
        </excludedMetrics>
        <excludedProperties>
          <string>commit-id</string>
          <string>yammer-metrics-count</string>
        </excludedProperties>
        <predefinedTags/>
        <putType>PROPERTY</putType>
        <entitySet reference="../../com.axibase.collector.model.job.jmx.JmxConfiguration/entitySet"/>
      </com.axibase.collector.model.job.jmx.JmxConfiguration>
    </configurations>
  </com.axibase.collector.model.job.jmx.JmxJobConfig>
</list>
